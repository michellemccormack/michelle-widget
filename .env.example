# Airtable
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_base_id

# Upstash Redis
UPSTASH_REDIS_REST_URL=your_redis_url
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Optional: Web search (Serper.dev) - enables answering questions not in FAQ
# Get free API key at https://serper.dev - 2,500 free searches/month
# SERPER_API_KEY=your_serper_api_key
# WEB_SEARCH_ENABLED=true  # Override Airtable config if cache is stale

# Security
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
RATE_LIMIT_MAX=20
RATE_LIMIT_WINDOW_MS=60000
SYNC_TOKEN=change-me-in-production

# Widget
NEXT_PUBLIC_WIDGET_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Optional: Logging
LOG_LEVEL=info
